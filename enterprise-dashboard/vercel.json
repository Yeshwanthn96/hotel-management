{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/enterprise-dashboard",
  "devCommand": "ng serve --port $PORT",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://localhost:8080/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
